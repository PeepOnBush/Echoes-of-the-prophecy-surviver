config_version=5

[application]

config/name="FrierenTheJourneyBeyond"
run/main_scene="res://Menu/FrierenMenu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://00_GlobalScripts/GlobalAudioManager.gd"
LevelManager="*res://00_GlobalScripts/GlobalLevelManager.gd"
PlayerHud="*res://GUI/player_hud/player_hud.tscn"
PlayerManager="*res://00_GlobalScripts/GlobalPlayerManager.gd"
SceneTransition="*res://GUI/scene_transition/scene_transition.tscn"
SaveManager="*res://00_GlobalScripts/GlobalSaveManager.gd"
PauseMenu="*res://GUI/pause_menu/pause_menu.tscn"
DialogSystem="*res://GUI/dialog_system/dialog_system.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1600
window/size/window_height_override=900
window/stretch/mode="viewport"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

